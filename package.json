{
	"name": "jekyll-n-hyde",
	"version": "0.0.1",
	"publisher": "Hepheir",
	"engines": {
		"vscode": "^1.65.0"
	},
	"displayName": "Jekyll n Hyde",
	"description": "Jekyll n Hyde :: is a lightweight post manager for Jekyll repository.",
	"categories": [
		"SCM Providers",
		"Other"
	],
	"keywords": [
		"jekyll"
	],
	"icon": "images/icon.png",
	"bugs": {
	  "email": "hepheir@gmail.com"
	},
	"repository": {
	  "type": "git",
	  "url": "https://github.com/Hepheir/vscode-jekyll-n-hyde"
	},
	"activationEvents": [
	],
	"main": "./out/extension.js",
	"contributes": {
		"viewsContainers": {
			"activitybar": [
				{
					"id": "jekyll-n-hyde",
					"title": "Jekyll & Hyde",
					"icon": "images/jekyll.svg"
				}
			]
		},
		"views": {
			"jekyll-n-hyde": [
			]
		},
		"commands": [
			{
				"command": "showTextDocument",
				"title": "Open Text Document"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.1.0",
		"@types/node": "14.x",
		"@types/vscode": "^1.65.0",
		"@typescript-eslint/eslint-plugin": "^5.12.1",
		"@typescript-eslint/parser": "^5.12.1",
		"@vscode/test-electron": "^2.1.2",
		"eslint": "^8.9.0",
		"glob": "^7.2.0",
		"mocha": "^9.2.1",
		"typescript": "^4.5.5"
	},
	"dependencies": {
		"@types/yamljs": "^0.2.31",
		"vsce": "^2.7.0",
		"yamljs": "^0.3.0"
	}
}
